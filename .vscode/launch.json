    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug test",
            "type": "go",
            "request": "launch",
            "program": "${fileDirname}",
            "buildFlags": "-gcflags='all=-N -l'",
            "showLog": true
        },
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "cmd/server/main.go",
            "env": {
                "APP_ENV": "development",
            },
            "cwd": "${workspaceFolder}",
        }
    ]
}